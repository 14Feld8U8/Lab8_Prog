<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab8.Controllers.SpaceMarineDialogController">
    <Label layoutX="50.0" layoutY="29.0" text="%name" textAlignment="CENTER" AnchorPane.leftAnchor="50.0"
           AnchorPane.rightAnchor="50.0"/>
    <TextField fx:id="nameField" layoutX="50.0" layoutY="46.0" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="79.0" text="%coordX" textAlignment="CENTER"
           AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
    <TextField fx:id="coordinateXField" layoutX="50.0" layoutY="96.0" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="129.0" text="%coordY" textAlignment="CENTER"
           AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
    <TextField fx:id="coordinateYField" layoutX="50.0" layoutY="146.0" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="179.0" text="%health"
           textAlignment="CENTER" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
    <TextField fx:id="healthField" layoutX="50.0" layoutY="196.0" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="229.0" text="%heartCount" textAlignment="CENTER"
           AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
    <TextField fx:id="heartCountField" layoutX="50.0" layoutY="246.0" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="279.0" text="%loyal" textAlignment="CENTER"
           AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
    <ComboBox fx:id="loyalBox" layoutX="68.0" layoutY="296.0" prefWidth="150.0" AnchorPane.leftAnchor="50.0"
              AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="329.0" text="%meleeWeapon" textAlignment="CENTER"
           AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"/>
    <ComboBox fx:id="meleeWeaponBox" layoutX="78.0" layoutY="346.0" prefWidth="150.0" AnchorPane.leftAnchor="50.0"
              AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="379.0" text="%chapterName" textAlignment="CENTER" AnchorPane.leftAnchor="50.0"
           AnchorPane.rightAnchor="50.0"/>
    <TextField fx:id="chapterNameField" layoutX="50.0" layoutY="396.0" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0"/>
    <Label layoutX="50.0" layoutY="429.0" text="%chapterWorld" textAlignment="CENTER" AnchorPane.leftAnchor="50.0"
           AnchorPane.rightAnchor="50.0"/>
    <TextField fx:id="chapterWorldField" layoutX="50.0" layoutY="446.0" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0"/>
    <Button layoutX="338.0" layoutY="492.0" mnemonicParsing="false" onAction="#OKClick" text="ОК"/>
    <Button layoutX="274.0" layoutY="492.0" mnemonicParsing="false" onAction="#CancelClick" text="%cancel"/>
</AnchorPane>
